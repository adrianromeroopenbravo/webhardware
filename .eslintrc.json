{
  "env": {
    "browser": true,
    "es6": true
  },
  "extends": ["eslint:recommended", "prettier"],
  "parserOptions": {
    "ecmaVersion": 9,
    "sourceType": "module"
  },
  "globals": {
  },
  "rules": {
    "eqeqeq": ["warn", "smart"],
    "no-unused-vars": [
      "warn",
      {
        "args": "none"
      }
    ],
    "no-console": ["warn"],
    "no-useless-escape": ["warn"]
  }
}
